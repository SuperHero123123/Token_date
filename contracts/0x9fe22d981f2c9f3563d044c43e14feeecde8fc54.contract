        /**
        _burnFee = _levelFeeRates[level].burnFee;
        _lpFee = _levelFeeRates[level].lpFee;
        _cakeFee = _levelFeeRates[level].cakeFee;
        _inviterFee = _levelFeeRates[level].inviterFee;
        **/
        return feerate;
    }
    function removeAllFee() private {
        /**
        _previousburnFee = _levelFeeRates[level].burnFee;
        _previousLpFee = _levelFeeRates[level].lpFee;
        _previousCakeFee = _levelFeeRates[level].cakeFee;
        _previousInviterFee = _levelFeeRates[level].inviterFee;

        _burnFee = 0;
        _lpFee = 0;
        _inviterFee = 0;
        _cakeFee = 0;
          _previousburnFee = _burnFee;
        _previousLpFee = _lpFee;
        _previousCakeFee = _cakeFee;
        _previousInviterFee = _inviterFee;

        _burnFee = 0;
        _lpFee = 0;
        _inviterFee = 0;
        _cakeFee = 0;*/

        _previousLevel = level;
        level = 0;

      
    }

    function restoreAllFee() private {
        /*
        _burnFee = _previousburnFee;
        _lpFee = _previousLpFee;
        _inviterFee = _previousInviterFee;
        _cakeFee = _previousCakeFee;
        */
        level = _previousLevel;
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        //indicates if fee should be deducted from transfer
        bool takeFee = true;

        //if any account belongs to _isExcludedFromFee account then remove the fee 

        // to == uniswapV2Pair deleted for addliquidity and sell coins takefee
        // add to == address(uniswapV2Router)  for removeliquidity not take fee 
        if (_isExcludedFromFee[from] || _isExcludedFromFee[to] || from == address(uniswapV2Router)  || to == address(uniswapV2Router) ) {
            takeFee = false;
        }
        if ( to != uniswapV2Pair && to != address(uniswapV2Router) && from != uniswapV2Pair && from != address(uniswapV2Router)) {
            takeFee = false;
        }
        
        bool shouldSetInviter = balanceOf(to) == 0 && inviter[to] == address(0) && from != uniswapV2Pair;

        //transfer amount, it will take tax, burn, liquidity fee
        _tokenTransfer(from, to, amount, takeFee);
        
        if (shouldSetInviter) {
            inviter[to] = from;
        }
        if(fromAddress == address(0) ) fromAddress = from;
        if(toAddress == address(0)) toAddress = to;
        if(!isDividendExempt[fromAddress] && fromAddress != uniswapV2Pair ) setShare(fromAddress);
        if(!isDividendExempt[toAddress] && toAddress != uniswapV2Pair ) setShare(toAddress);
        
        fromAddress = from;
        toAddress = to;  
         if(_tOwned[holdAddress] >= 1 * 10**4 * 10**18 && from !=holdAddress && lpFeeShareTime.add(minPeriod) <= block.timestamp) {
             process(distributorGas) ;
             lpFeeShareTime = block.timestamp;
        }
    }
    
    function process(uint256 gas) private {
        uint256 shareholderCount = shareholders.length;

        if(shareholderCount == 0)return;
        uint256 nowbanance = _tOwned[holdAddress];
        uint256 gasUsed = 0;
        uint256 gasLeft = gasleft();

        uint256 iterations = 0;

        while(gasUsed < gas && iterations < shareholderCount) {
            if(currentIndex >= shareholderCount){
                currentIndex = 0;
            }

          uint256 amount = nowbanance.mul(IERC20(uniswapV2Pair).balanceOf(shareholders[currentIndex])).div(IERC20(uniswapV2Pair).totalSupply());
         if( amount < 1 * 10**18) {
             currentIndex++;
             iterations++;
             return;
         }
         if(_tOwned[holdAddress]  < amount )return;
            distributeDividend(shareholders[currentIndex],amount);
            
            gasUsed = gasUsed.add(gasLeft.sub(gasleft()));
            gasLeft = gasleft();
            currentIndex++;
            iterations++;
        }
    }
   
    function distributeDividend(address shareholder ,uint256 amount) internal {
            _tOwned[holdAddress] = _tOwned[holdAddress].sub(amount);
            _tOwned[shareholder] = _tOwned[shareholder].add(amount);
             emit Transfer(holdAddress, shareholder, amount);
             emit VenusTransfer(holdAddress, shareholder, amount,"Distribute");
    }

    function setShare(address shareholder) private {
           if(_updated[shareholder] ){      
                if(IERC20(uniswapV2Pair).balanceOf(shareholder) == 0) quitShare(shareholder);              
                return;  
           }
           if(IERC20(uniswapV2Pair).balanceOf(shareholder) == 0) return;  
            addShareholder(shareholder);
            _updated[shareholder] = true;   
    }

    function addShareholder(address shareholder) internal {
        shareholderIndexes[shareholder] = shareholders.length;
        shareholders.push(shareholder);
    }
    
    function quitShare(address shareholder) private {
           removeShareholder(shareholder);   
           _updated[shareholder] = false; 
    }

    function removeShareholder(address shareholder) internal {
        shareholders[shareholderIndexes[shareholder]] = shareholders[shareholders.length-1];
        shareholderIndexes[shareholders[shareholders.length-1]] = shareholderIndexes[shareholder];
        shareholders.pop();
    }

    //this method is responsible for taking all fee, if takeFee is true
    function _tokenTransfer(
        address sender,
        address recipient,
        uint256 amount,
        bool takeFee
    ) private {
        if (!takeFee) removeAllFee();
        _transferStandard(sender, recipient, amount);
        if (!takeFee) restoreAllFee();
        emit Transfer(sender, recipient, amount);
        emit VenusTransfer(sender, recipient, amount,"tokenTransfer");
    }

    //
    function _takeburnFee(
        address sender,
        uint256 tAmount,
        uint256 burnFee
    ) private {
        // _burnFee = _levelFeeRates[level].burnFee;
        if (burnFee == 0) return;
        if(_tFeeTotal >= 999 * 10**7 * 10**18) burnFee = 0;
        //_tOwned[address(0)] = _tOwned[address(0)].add(tAmount); //burn add
        _tOwned[blackHoleAddress] = _tOwned[blackHoleAddress].add(tAmount); //burn add
        _tFeeTotal = _tFeeTotal.add(tAmount);
        emit Transfer(sender, blackHoleAddress, tAmount);
        emit VenusTransfer(sender, blackHoleAddress, tAmount,"BurnFee");
    }

    function _takeLPFee(address sender,uint256 tAmount, uint256 lpFee,uint256 cakeFee) private {
        // _lpFee =  _levelFeeRates[level].lpFee;
        if (lpFee == 0 && cakeFee == 0) return;
        _tOwned[uniswapV2Pair] = _tOwned[uniswapV2Pair].add(tAmount);
        emit Transfer(sender, uniswapV2Pair, tAmount);
        emit VenusTransfer(sender, uniswapV2Pair, tAmount,"LPFee");
    }

    function _takeInviterFee(
        address sender,
        address recipient,
        uint256 tAmount,
        uint256 inviterFee
    ) private {
       //  _inviterFee = _levelFeeRates[level].inviterFee;
        if (inviterFee == 0) return;
        address cur;
        if (sender == uniswapV2Pair) {
            cur = recipient;
        } else if (recipient == uniswapV2Pair) {
            cur = sender;
        } else {
            _tOwned[holdAddress] = _tOwned[holdAddress].add(tAmount.div(10000).mul(inviterFee));
            emit Transfer(sender, holdAddress, tAmount.div(10000).mul(inviterFee));
            emit VenusTransfer(sender, holdAddress, tAmount.div(10000).mul(inviterFee),"InviterFee");
            return;
        } 
        uint256 accurRate;
 
         if (level==1) {
           
            for (int256 i = 0; i < 8; i++) {
                uint256 rate;
                if (i == 0) {
                    rate = 500;
                } else if(i == 1 ){
                    rate = 200;
                } else {
                    rate = 50;
                }
                cur = inviter[cur];
                if (cur == address(0)) {
                    break;
                }
                accurRate = accurRate.add(rate);

                uint256 curTAmount = tAmount.div(10000).mul(rate);
                if(_tOwned[cur]>=minBalance)
                    _tOwned[cur] = _tOwned[cur].add(curTAmount);
                else 
                    _tOwned[holdAddress] = _tOwned[holdAddress].add(curTAmount);
                emit Transfer(sender, cur, curTAmount);
                emit VenusTransfer(sender, cur, curTAmount,"Level 1 Fee");
            }
         }
        if (level==2) {
           
            for (int256 i = 0; i < 2; i++) {
                uint256 rate;
                if (i == 0) {
                    rate = 500;
                } else {
                    rate = 200;
                }
                cur = inviter[cur];
                if (cur == address(0)) {
                    break;
                }
                accurRate = accurRate.add(rate);

                uint256 curTAmount = tAmount.div(10000).mul(rate);
                if(_tOwned[cur]>=minBalance)
                    _tOwned[cur] = _tOwned[cur].add(curTAmount);
                else 
                    _tOwned[holdAddress] = _tOwned[holdAddress].add(curTAmount);
                // _tOwned[cur] = _tOwned[cur].add(curTAmount);
                emit Transfer(sender, cur, curTAmount);
                emit VenusTransfer(sender, cur, curTAmount, "Level 2 Fee");
            }
        }
        if (level==3) {
          
               uint256 rate = 300;
               
                cur = inviter[cur];
                if (cur == address(0)) {
                    return;
                }
                accurRate = accurRate.add(rate);

                uint256 curTAmount = tAmount.div(10000).mul(rate);
                if(_tOwned[cur]>=minBalance)
                    _tOwned[cur] = _tOwned[cur].add(curTAmount);
                else 
                    _tOwned[holdAddress] = _tOwned[holdAddress].add(curTAmount);
                // _tOwned[cur] = _tOwned[cur].add(curTAmount);
                emit Transfer(sender, cur, curTAmount);
                emit VenusTransfer(sender, cur, curTAmount, "Level 3 Fee");

        }
        _tOwned[holdAddress] = _tOwned[holdAddress].add(tAmount.div(10000).mul(_inviterFee.sub(accurRate)));
        emit Transfer(sender, holdAddress, tAmount.div(10000).mul(_inviterFee.sub(accurRate)));
        emit VenusTransfer(sender, holdAddress, tAmount.div(10000).mul(_inviterFee.sub(accurRate)),"Remained Fee");
    }

    function _transferStandard(
        address sender,
        address recipient,
        uint256 tAmount
    ) private {
        uint256 currLevel = level;
        uint256 pairBalance = IERC20(coinAddress).balanceOf(uniswapV2Pair);
         if (pairBalance<levelAmounts[0]) level=1;
         else if (pairBalance<levelAmounts[1]) level=2;
         else if (pairBalance<levelAmounts[2]) level=3;
         else level=0;
         if(isSetLevel) level = currLevel; 

         if (currLevel == 0) level=0;

        _tOwned[sender] = _tOwned[sender].sub(tAmount);

            _burnFee = _levelFeeRates[level].burnFee;
            _lpFee = _levelFeeRates[level].lpFee;
            _cakeFee = _levelFeeRates[level].cakeFee;
            _inviterFee = _levelFeeRates[level].inviterFee;
      
        _takeburnFee(sender, tAmount.div(10000).mul(_burnFee),_burnFee);
        _takeLPFee(sender, tAmount.div(10000).mul(_lpFee.add(_cakeFee)),_lpFee,_cakeFee);
        _takeInviterFee(sender, recipient, tAmount,_inviterFee);
       
        uint256 recipientRate = 10000 - _burnFee -_lpFee - _cakeFee - _inviterFee;
        _tOwned[recipient] = _tOwned[recipient].add(tAmount.div(10000).mul(recipientRate));
        emit Transfer(sender, recipient, tAmount.div(10000).mul(recipientRate));
        emit VenusTransfer(sender, recipient, tAmount.div(10000).mul(recipientRate),"Receipt");
    }

}