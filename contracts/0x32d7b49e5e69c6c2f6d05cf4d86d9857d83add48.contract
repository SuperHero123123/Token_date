pragma solidity ^0.7.6;

contract PeaERC20 is Ownable, ERC20 {
    using SafeMath for uint256;

    uint256 internal capFarm = 200 * 10**6 * 10**18;
    uint256 private farmRewarded;

    uint256 internal capCombat = 200 * 10**6 * 10**18;
    uint256 private combatRewarded;

    address public farmer;
    address public arena;

    uint256 public tokenForBosses = 1;

    uint256 public antiBotBuyAmount;
    uint256 public userLimitAccumulateAmount;
    uint256 public userSellLimitTime;

    mapping(address => uint256) public lastActionBlock;
    mapping(address => uint256) public userSellTime;
    mapping(address => bool) internal whiteList;

    address public addressForBosses;
    uint256 public sellFeeRate = 6;
    uint256 public buyFeeRate = 0;

    uint256 public userSellLimitPercent;
    uint256 public userSellMinPermit;

    constructor(string memory name, string memory symbol) ERC20(name, symbol) {
        addressForBosses = _msgSender();
        userSellLimitTime = 1 days;
        userSellLimitPercent = 20;
        userSellMinPermit = 30 * 10**3 * 10**18;
        antiBotBuyAmount = 475 * 10**3 * 10**18;
        userLimitAccumulateAmount = 720 * 10**3 * 10**18;
    }

    modifier onlyFarmer() {
        require(farmer == _msgSender(), "caller is not the farmer");
        _;
    }

    modifier onlyArena() {
        require(arena == _msgSender(), "caller is not the arena");
        _;
    }

    function setWhiteList(address addr, bool isWhite) public onlyOwner {
        whiteList[addr] = isWhite;
    }

    function setFarmer(address _farmer) public onlyOwner {
        farmer = _farmer;
    }

    function setArena(address _arena) public onlyOwner {
        arena = _arena;
    }

    function setSellStrategy(uint256 _userSellLimitTime, uint256 _userSellLimitPercent, uint256 _userSellMinPermit) public onlyOwner {
        userSellLimitTime = _userSellLimitTime;
        userSellLimitPercent = _userSellLimitPercent;
        userSellMinPermit = _userSellMinPermit;
    }

    function setTransferFeeRate(uint256 _sellFeeRate, uint256 _buyFeeRate) public onlyOwner {
        sellFeeRate = _sellFeeRate;
        buyFeeRate = _buyFeeRate;
    }

    function setBotGuardAmount(uint256 _buyMaxAmount, uint256 _userLimitAccumulateAmount) public onlyOwner {
        antiBotBuyAmount = _buyMaxAmount * 10**18;
        userLimitAccumulateAmount = _userLimitAccumulateAmount * 10**18;
    }

    function setMinTokensBeforeSwap(uint256 _tokenForBosses) public onlyOwner {
        require(_tokenForBosses < 20 * 10**6 * 10**18);
        tokenForBosses = _tokenForBosses;
    }

    function combatReward(address recipient, uint256 _amount)
        external
        onlyArena
    {
        require(capCombat > combatRewarded, "Over cap combat reward");
        require(recipient != address(0), "0x is not accepted here");
        require(_amount > 0, "can not reward 0 token");

        combatRewarded = combatRewarded.add(_amount);
        if (combatRewarded <= capCombat) {
            _mint(recipient, _amount);
        } else {
            uint256 availableReward = combatRewarded.sub(capCombat);
            _mint(recipient, availableReward);
            combatRewarded = capCombat;
        }
    }

    function farm(address recipient, uint256 amount) external onlyFarmer {
        require(capFarm > farmRewarded, "Over cap farm");
        require(recipient != address(0), "0x is not accepted here");
        require(amount > 0, "not accept 0 value");

        farmRewarded = farmRewarded.add(amount);
        if (farmRewarded <= capFarm) {
            _mint(recipient, amount);
        } else {
            uint256 availableReward = farmRewarded.sub(capFarm);
            _mint(recipient, availableReward);
            farmRewarded = capFarm;
        }
    }
}

pragma solidity ^0.7.6;

contract Pea is PeaERC20, ReentrancyGuard {
    using SafeMath for uint256;
    using Address for address;

    mapping(address => bool) bots;
    bool internal isAddLiquidity;
    uint256 internal addLiquidityBlock;
    uint256 public maxSupply = 1 * 10**9 * 10**18;

    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;

    bool private botGuard;

    constructor(string memory name, string memory symbol)
        PeaERC20(name, symbol)
    {
        _mint(_msgSender(), maxSupply.sub(capFarm).sub(capCombat));
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x10ED43C718714eb63d5aA57B78B54704E256024E //Pancake Swap Router
            // 0x5BBc6a9Fe29785C9e75EC9c90855a8384499Aef1 //Pancake Swap Router
        );

        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        _approve(address(this), address(uniswapV2Router), ~uint256(0));

        botGuard = true;
        isAddLiquidity = true;
        addLiquidityBlock = 0;
    }

    function burn(uint256 amount) public {
        _burn(_msgSender(), amount);
    }

    function activeSetLiquidity() public onlyOwner {
        require(!isAddLiquidity, "addliquidity currently ons");
        isAddLiquidity = true;
        addLiquidityBlock = 0;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override {
        uint256 transferFeeRate = 0;
        bool isBuy = sender == uniswapV2Pair;
        bool isSell = recipient == uniswapV2Pair;
        // bool isTransfer = !(isBuy || isSell);
        address _user = isSell ? sender : recipient;
        uint256 _userBalance;
        uint256 _userTransferPermit;

        if (botGuard) {
            if (isBuy) {
                transferFeeRate = 0;
                if (!_user.isContract()) { // will not block swaper contract
                    require(lastActionBlock[_user] != block.number, "PEA: Can not buy twice in one block"); //buy too fast
                    lastActionBlock[_user] = block.number; // user last action timestamp
                }
            } else if (isSell) {
                transferFeeRate = 17; //dont sell when presale
            }

            if (isAddLiquidity) // will not block transfer when add liquidity
            {
                require(addLiquidityBlock == 0, "addLiquidityBlock is not ready"); // addLiquidityBlock is not setted before
                addLiquidityBlock = block.number; // moment add liquidity
                isAddLiquidity = false; // start block immediately
                transferFeeRate = 0; // no transfer fee for liquidity
            }
            else
            {
                require(amount <= antiBotBuyAmount, "PEA: OVER_MAX_AMOUNT_TRANSACTION"); // no whale accepted here
                if (
                    !whiteList[_user] && // farming, pve, pvp contract will not block transfer, can not hardcode since havent deployed
                    _user != address(this) && // no self transfer
                    !isSell && //block whale from transfer and buy
                    !_user.isContract() // will not block swaper contract
                ) {
                    _userBalance = balanceOf(_user);
                    if (_userBalance.add(amount) > userLimitAccumulateAmount) {
                        revert("OVER_MAX_BUY_AMOUNT_USER");
                    }
                }
            }
        }
        else
        {   // fee after botGuard
            transferFeeRate = isSell ? sellFeeRate
                                     : (isBuy ? buyFeeRate : 0); // no fee for transfer
        }

        if (
            !whiteList[recipient] && 
            !whiteList[_user] && 
            !isBuy && 
            !_user.isContract()
        ) {
            require(lastActionBlock[_user] != block.number, "PEA: Can not receive Pea twice in a block");
            if (isSell) {
                require(userSellTime[_user] < block.timestamp - userSellLimitTime, "PEA: can not sell now");
                userSellTime[_user] = block.timestamp;
            }
            _userBalance = balanceOf(_user);
            _userTransferPermit = _userBalance * userSellLimitPercent / 100;
            _userTransferPermit = _userTransferPermit < userSellMinPermit ? userSellMinPermit : _userTransferPermit;
            if (amount > _userTransferPermit) {
                revert("PEA: OVER_MAX_TRANSFER_AMOUNT_USER");
            }
            lastActionBlock[_user] = block.number;
        }

        if (
            transferFeeRate > 0 &&
            sender != address(this) &&
            recipient != address(this)
        ) {
            uint256 _fee = amount.mul(transferFeeRate).div(100);
            super._transfer(sender, address(this), _fee); // TransferFee
            amount = amount.sub(_fee);
        }

        super._transfer(sender, recipient, amount);
    }

    function sweepTokenForBosses() public nonReentrant {
        uint256 contractTokenBalance = balanceOf(address(this));
        if (contractTokenBalance >= tokenForBosses) {
            swapTokensForEth(tokenForBosses);
        }
    }

    function sweepTokenForBosses(uint256 _amount) public nonReentrant {
        setMinTokensBeforeSwap(_amount);
        uint256 contractTokenBalance = balanceOf(address(this));
        if (contractTokenBalance >= _amount) {
            swapTokensForEth(_amount);
        }
    }

    // receive eth from uniswap swap
    receive() external payable {}

    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            addressForBosses, // The contract
            block.timestamp //600 seconds
        );
    }

    function setAddressForBosses(address _bossesAddress) external onlyOwner {
        require(_bossesAddress != address(0), "0x is not accepted here");

        addressForBosses = _bossesAddress;
    }

    function stopBotGuard() public onlyOwner {
        botGuard = false;
        userSellLimitPercent = 30;
    }

    function activateBotGuard() public onlyOwner {
        botGuard = true;
        userSellLimitPercent = 20;
    }
}